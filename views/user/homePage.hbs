          <div class="container">
            <div class="row align-items-center slider-animated-1">
              <div class="col-lg-5 col-md-6">
                <div class="hero-slider-content-2">
                  <h4 class="animated">{{this.titleSmall}}</h4>
                  <h2 class="animated fw-900">{{this.titleMain}}</h2>
                  <h1 class="animated fw-900 text-brand">{{this.highlight}}</h1>
                  <p class="animated">{{this.description}}</p>
                  <a
                    class="animated btn btn-brush btn-brush-3"
                    href="shop-product-right.html"
                  > Shop Now </a>
                </div>
              </div>
              <div class="col-lg-7 col-md-6">
                <div class="single-slider-img single-slider-img-1">
                  <img
                    class="animated slider-1-1"
                    src="{{this.image}}"
                    alt=""
                  />
                </div>
              </div>
            </div>
          </div>
        </div>
      {{/each}}
